#ifndef BLINKTORADIO_H
#define BLINKTORADIO_H

enum {
  AM_BLINKTORADIOMSG = 6,
  TIMER_PERIOD_MILLI = 40
};

typedef nx_struct BlinkToRadioMsg {
  nx_uint32_t nodeid;
  nx_uint32_t counter;
  nx_uint32_t sendts;
  nx_uint32_t receivets;
  nx_uint32_t resendts;
  nx_uint32_t endts;
  
} BlinkToRadioMsg;

#endif
